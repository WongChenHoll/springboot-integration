<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jason.mybatisplus.project.mapper.UserMapper">

    <select id="pageList" resultType="com.jason.mybatisplus.project.model.UserModel">
